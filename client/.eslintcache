ctives/client/src/pages/BoardList.js",["76","77"],"import '../index.css'\n\nimport React, { useState } from 'react';\n\nimport { useQuery } from '@apollo/client';\nimport { QUERY_BOARDS } from '../utils/queries';\n\nimport { useMutation } from '@apollo/client';\nimport { ADD_BOARD } from '../utils/mutations';\n\n\nconst BoardList = () => {\n    const { data } = useQuery(QUERY_BOARDS);\n    const boards = data?.boards || [];\n\n    const [name, setName] = useState('');\n  \n    const [addBoard, { error }] = useMutation(ADD_BOARD);\n  \n    const handleFormSubmit = async (event) => {\n      event.preventDefault();\n  \n      try {\n        const { data } = await addBoard({\n          variables: { name },\n        });\n  \n        window.location.reload();\n      } catch (err) {\n        console.error(err);\n      }\n    };\n\n    return(\n        <main className=\"container\">\n            <div >\n                <h2 className=\"container-header\">Retrospectives</h2>\n                <div className=\"sections\">\n                    <form onSubmit={handleFormSubmit}>\n                        <input \n                            name=\"name\"\n                            value={name}\n                            onChange={(event) => setName(event.target.value)}\n                        >\n                        </input>\n                        <button className=\"add-button\" type=\"submit\">\n                            + Create board\n                        </button>\n                    </form>\n                    <div className=\"listed-boards\">\n                        <h3>Your boards</h3>\n                        <ul className=\"boards-list\">\n                            {boards &&\n                            boards.map((boards) => (\n                            <li key={boards._id}>\n                                {boards.name}\n                            </li>\n                        ))}\n                        </ul>\n                    </div>\n    