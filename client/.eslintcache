 there are no items, return empty array\n  const items = data?.items || [];\n\n  // Use mutation to remove item using it's ID\n  const [removeItem, { error }] = useMutation(REMOVE_ITEM)\n  const handleRemoveItem = async (id) => {\n    try {\n      const { event } = await removeItem({\n        variables: { itemId: id },\n      });\n\n      window.location.reload();\n\n    } catch (err) {\n      console.error(err);\n    }\n  };\n  \n  // Filter the items based on category to populate relevent sections\n  function filterCat01() {\n    return items.filter(\n      (items) => items.category === \"01\"\n    )\n  }\n  function filterCat02() {\n    return items.filter(\n      (items) => items.category === \"02\"\n    )\n  }\n  function filterCat03() {\n    return items.filter(\n      (items) => items.category === \"03\"\n    )\n  }\n  \n  // Display retro board, with sections and corresponding items\n    return (\n      <main className=\"board\">\n        <div className=\"container\">\n          <div className=\"container-header\">\n            <h2>Sprint 3 retrospective</h2>\n            <div className=\"aside-components\">\n            <AddModal />\n          </div>\n          </div>\n          <div className=\"sections\">\n            <div className=\"section\">\n                <h3 className=\"section-heading\">What went well?</h3>\n                <div className=\"section-area\">\n                  <ul className=\"item-card-list\">\n                    {filterCat01().map((items) => (\n                      <li className=\"item-card\"\n                        key={items._id}\n                      >{items.text}\n                        <button className=\"deleteButton\"\n                          name=\"itemId\"\n                          value={items._id}\n                          onClick={() => handleRemoveItem(items._id)} >\n                          x\n                        </button>\n                      </li>\n                    ))}\n                  </ul>\n                </div>\n            </div>\n            <div className=\"section\">\n                <h3 className=\"section-heading\">What didn't go well?</h3>\n                <div className=\"section-area\">\n                <ul className=\"item-card-list\">\n                    {filterCat02().map((items) => (\n                      <li className=\"item-card\"\n                      key={items._id}\n                    >{items.text}\n                      <button className=\"deleteButton\"\n                          name=\"itemId\"\n                          value={items._id}\n                          onClick={() => handleRemoveItem(items._id)} >\n                          x\n                        </button>\n                    </li>\n                    ))}\n                  </ul>\n                </div>\n            </div>\n            <div className=\"section\">\n                <h3 className=\"section-heading\">What confuses you?</h3>\n                <div className=\"section-area\">\n                  <ul className=\"item-card-list\">\n                    {filterCat03().map((items) => (\n                      <li className=\"item-card\"\n                      key={items._id}\n                    >{items.text}\n                      <button className=\"deleteButton\"\n                          name=\"itemId\"\n                          value={items._id}\n                          onClick={() => handleRemoveItem(items._id)} >\n                          x\n                        </button>\n                    </li>\n                    ))}\n                  </ul>\n                </div>\n            </div>\n          </div> \n        </div>\n        <div className=\"aside-components\">\n          <Actions />\n        </div>\n      </main>\n    );\n  };\n\nexport default Board;","/Users/rachelharrispersonal/team-retrospectives/client/src/utils/queries.js",[],"/Users/rachelharrispersonal/team-retrospectives/client/src/pages/Login.js",["80"],"/Users/rachelharrispersonal/team-retrospectives/client/src/pages/Signup.js",[],"/Users/rachelharrispersonal/team-retrospectives/client/src/utils/auth.js",[],"/Users/rachelharrispersonal/team-retrospectives/client/src/utils/mutations.js",[],"/Users/rachelharrispersonal/team-retrospectives/client/src/components/Header.js",[],"/Users/rachelharrispersonal/team-retrospectives/client/src/components/AddModal.js",[],"/Users/rachelharrispersonal/team-retrospectives/client/src/components/Modal.js",["81","82"],"import './Modal.css';\nimport '../index.css'\n\nimport React, { useState } from 'react';\nimport { useMutation } from '@apollo/client';\n\nimport { ADD_ITEM } from '../utils/mutations';\n\nconst Modal = ({ handleClose, handleSave, show, children }) => {\n  const showHideClassName = show ? \"modal display-block\" : \"modal display-none\";\n\n  const [text, setText] = useState('');\n  const [category, setCategory] = useState('');\n\n  const [addItem, { error }] = useMutation(ADD_ITEM);\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await addItem({\n        variables: { text, category },\n      });\n\n      window.location.reload();\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  return (\n    <div className={showHideClassName}>\n      <section className=\"modal-main\">\n        {children}\n        <h3>Your thoughts</h3>\n        <form onSubmit={handleFormSubmit}>\n          <label>Category</label>\n          <select\n            name=\"category\"\n            defaultValue={\"default\"}\n            onChange={(event) => setCategory(event.target.value)}\n          >\n            <option value=\"default\" disabled>\n              Select a category... \n            </option>\n            <option value=\"01\">What went well?</option>\n            <option value=\"02\">What didn't go well?</option>\n            <option value=\"03\">What confuses you?</option>\n          </select>\n          <textarea className=\"thought-text-area\"\n            name=\"text\"\n            placeholder=\"What's on your mind..\"\n            value={text}\n            onChange={(event) => setText(event.target.value)}\n            rows=\"4\"\n            cols=\"50\"\n          >\n          </textarea>\n          <div className=\"action-buttons\">\n            <button type=\"submit\" className=\"button-save\" onClick={handleSave}>\n              Save\n            </button>\n            <button type=\"button\" className=\"button-close\" onClick={handleClose}>\n              Cancel\n            </button>\n          </div>\n          \n        </form>\n      </section>\n    </div>\n  );\n};\n\nexport default Modal;","/Users/rachelharrispersonal/team-retrospectives/client/src/pages/Profile.js",[],"/Users/rachelharrispersonal/team-retrospectives/client/src/pages/BoardList.js",["83","84"],"import '../index.css'\n\nimport React, { useState } from 'react';\n\nimport { useQuery } from '@apollo/client';\nimport { QUERY_BOARDS } from '../utils/queries';\n\nimport { useMutation } from '@apollo/client';\nimport { ADD_BOARD } from '../utils/mutations';\n\n\nconst BoardList = () => {\n    const { data } = useQuery(QUERY_BOARDS);\n    const boards = data?.boards || [];\n\n    const [name, setName] = useState('');\n  \n    const [addBoard, { error }] = useMutation(ADD_BOARD);\n  \n    const handleFormSubmit = async (event) => {\n      event.preventDefault();\n  \n      try {\n        const { data } = await addBoard({\n          variables: { name },\n        });\n  \n        window.location.reload();\n      } catch (err) {\n        console.error(err);\n      }\n    };\n\n    return(\n        <main className=\"container\">\n            <div >\n                <h2 className=\"container-header\">Retrospectives</h2>\n                <div className=\"sections\">\n                    <form onSubmit={handleFormSubmit}>\n                        <input \n                            name=\"name\"\n                            value={name}\n                            onChange={(event) => setName(event.target.value)}\n                        >\n                        </input>\n                        <button className=\"add-button\" type=\"submit\">\n                            + Create board\n                        </button>\n                    </form>\n                    <div className=\"listed-boards\">\n                        <h3>Your boards</h3>\n                        <ul className=\"boards-list\">\n                            {boards &&\n                            boards.map((boards) => (\n                            <li key={boards._id}>\n                                {boards.name}\n                            </li>\n                        ))}\n                        </ul>\n                    </div>\n                </div>\n            </div>\n        </main>\n    );\n\n}\n\nexport default BoardList;","/Users/rachelharrispersonal/team-retrospectives/client/src/pages/Actions.js",[],"/Users/rachelharrispersonal/team-retrospectives/client/src/components/ActionList.js",["85","86"],"import './ActionList.css'\nimport '../index.css'\n\nimport React from 'react';\n\nimport { useMutation } from '@apollo/client';\nimport { REMOVE_ACTION } from '../utils/mutations';\n\nimport { useQuery } from '@apollo/client';\nimport { QUERY_ACTIONS } from '../utils/queries'\n\nconst ActionList = () => {\n\n    const { data } = useQuery(QUERY_ACTIONS);\n    const actions = data?.actions || [];\n\n    const [removeAction, { error }] = useMutation(REMOVE_ACTION)\n    const handleRemoveAction = async (id) => {\n        try {\n            const { event } = await removeAction({\n                variables: { actionId: id },\n            });\n\n            window.location.reload();\n        } catch (err) {\n            console.error(err);\n        }\n    }\n\n    return (\n        <main>\n            <div>\n                <h3>Actions</h3>\n                <ul className=\"actions-list\">\n        